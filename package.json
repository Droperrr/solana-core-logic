  "dependencies": {
    "@coral-xyz/anchor": "^0.31.1",
    "@solana/web3.js": "^1.98.2",
    "bs58": "^6.0.0",
    "fastify": "^5.4.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/bn.js": "^5.2.0",
    "@types/bs58": "^4.0.4",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.10",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "eslint": "^9.30.1",
    "jest": "^30.0.4",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.0",
    "typescript": "^5.8.3"
  }
